<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="90.0" prefWidth="411.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.view.admin.components.UserItemViewController">
   <children>
      <BorderPane prefHeight="18.0" prefWidth="411.0">
         <padding>
            <Insets left="10.0" right="10.0" top="10.0" />
         </padding>
         <top>
            <HBox prefHeight="12.0" prefWidth="401.0" spacing="4.0" BorderPane.alignment="CENTER">
               <children>
                  <TextField fx:id="emailTextField" prefHeight="25.0" promptText="email" />
                  <TextField fx:id="passwordTextField" prefHeight="25.0" promptText="password" />
                  <ChoiceBox fx:id="roleChoiceBox" prefHeight="25.0" prefWidth="102.0" />
               </children>
               <BorderPane.margin>
                  <Insets bottom="4.0" />
               </BorderPane.margin>
            </HBox>
         </top>
         <center>
            <HBox alignment="CENTER_RIGHT" prefHeight="3.0" prefWidth="401.0" spacing="4.0" BorderPane.alignment="CENTER">
               <children>
                  <Label fx:id="userIdLabel" prefHeight="25.0" prefWidth="265.0" text="###">
                     <padding>
                        <Insets left="5.0" />
                     </padding></Label>
                  <Button mnemonicParsing="false" onAction="#updateUser" prefHeight="25.0" prefWidth="62.0" text="Update" />
                  <Button mnemonicParsing="false" onAction="#deleteUser" prefHeight="25.0" prefWidth="58.0" text="Delete" />
               </children>
            </HBox>
         </center>
         <bottom>
            <Separator prefWidth="411.0" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets bottom="10.0" top="20.0" />
               </BorderPane.margin>
            </Separator>
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
